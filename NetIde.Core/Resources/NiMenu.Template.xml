<?xml version="1.0" encoding="utf-8" ?>
<resources xmlns="https://github.com/pvginkel/NetIde/schemas/Resources">
  <imports>
    <import from="..\..\NetIde.Shell\bin\Debug\NetIde.Shell.dll" type="NetIde.Shell.NiResources" />
  </imports>
  <ui>
    <menu id="Menu_File" text="@MenuFile" priority="100" kind="Menu">
      <group priority="100">
        <button id="File_NewProject" priority="100" text="@MenuFileNewProject" key="Control+Shift+N" image="@DocumentsNew"/>
        <button id="File_OpenProject" priority="200" text="@MenuFileOpenProject" key="Control+Shift+O" image="@Documents"/>
      </group>
      <group priority="200">
        <button id="File_Close" priority="100" text="@MenuFileClose" key="Control+F4" />
        <button id="File_CloseProject" priority="200" text="@MenuFileCloseProject" image="@Exit" />
      </group>
      <group priority="300">
        <button id="File_Save" priority="100" text="@MenuFileSave" key="Control+S" image="@DiskBlue" />
        <button id="File_SaveAs" priority="200" text="@MenuFileSaveAs" />
        <button id="File_SaveAll" priority="300" text="@MenuFileSaveAll" key="Control+Shift+S" image="@Disks" />
      </group>
      <group priority="1000">
        <button id="File_Exit" priority="1000" text="@MenuFileExit" key="Alt+F4" image="@Exit" />
      </group>
    </menu>
    <menu id="Menu_Tools" text="@MenuTools" priority="1000" kind="Menu">
      <group priority="100">
        <button id="Tools_PackageManagement" priority="100" text="@MenuToolsPackageManagement"/>
      </group>
      <group priority="200">
        <button id="Tools_Options" priority="100" text="@MenuToolsOptions" />
      </group>
    </menu>
    <menu id="Menu_Window" text="@MenuWindow" priority="2000" kind="Menu">
      <group priority="1000">
        <button id="Window_CloseAllDocuments" priority="100" text="@MenuCloseAllDocuments" key="Control+Shift+F4" />
      </group>
    </menu>
  </ui>
</resources>
